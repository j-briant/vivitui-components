{
  "keybindings": {
    "Home": {
      "<q>": "Quit", // Quit the application
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend", // Suspend the application
      "<j>": "DownLayer",
      "down": "DownLayer",
      "<k>": "UpLayer",
      "up": "UpLayer",
      "<u>": "ScrollUp",
      "<d>": "ScrollDown",
      "left": "ScrollLeft",
      "right": "ScrollRight",
    },
    "LayerList": {
      "<q>": "Quit", // Quit the application
      "down": "DownLayer",
      "up": "UpLayer",
    },
    "Srs": {
      "<q>": "Quit", // Quit the application
      "down": "ScrollDown",
      "up": "ScrollUp",
      "left": "ScrollLeft",
      "right": "ScrollRight",
    },
  },
}
